{
  "extends": "./packages/tsconfig/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@bfra.me/badge-config": ["packages/badge-config/src"],
      "@bfra.me/badge-config/*": ["packages/badge-config/src/*"],
      "@bfra.me/create": ["packages/create/src"],
      "@bfra.me/create/*": ["packages/create/src/*"],
      "@bfra.me/es": ["packages/es/src"],
      "@bfra.me/es/*": ["packages/es/src/*"],
      "@bfra.me/eslint-config": ["packages/eslint-config/src"],
      "@bfra.me/eslint-config/*": ["packages/eslint-config/src/*"],
      "@bfra.me/prettier-config": ["packages/prettier-config/src"],
      "@bfra.me/prettier-config/*": ["packages/prettier-config/src/*"],
      "@bfra.me/semantic-release": ["packages/semantic-release/src"],
      "@bfra.me/semantic-release/*": ["packages/semantic-release/src/*"],
      "@bfra.me/tsconfig": ["packages/tsconfig"],
