{
  "name": "@bfra.me/cli",
  "description": "bfra.me command line",
  "version": "0.0.0",
  "author": "Marcus R. Brown @igetgames",
  "bin": {
    "bfra.me": "./bin/run"
  },
  "bugs": "https://github.com/bfra-me/works/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.0",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.25.1",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "globby": "^10.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bfra-me/works",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bfra.me",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bfra-me/works",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "eslintConfig": {
    "extends": [
      "oclif",
      "oclif-typescript"
    ]
  }
}
