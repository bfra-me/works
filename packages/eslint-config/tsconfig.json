{
  "$schema": "http://json.schemastore.org/tsconfig",
  "extends": "../tsconfig/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": false,
    "noPropertyAccessFromIndexSignature": false,
    "outDir": "./lib"
  },
  "include": ["src/**/*.ts", "package.json"],
  "exclude": ["node_modules", "lib"]
}
