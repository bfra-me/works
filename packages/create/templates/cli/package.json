{
  "name": "<%= it.name %>",
  "version": "1.0.0",
  "description": "<%= it.description %>",
  "keywords": [
    "cli",
    "command-line",
    "tool"
  ],
  "license": "MIT",
  "author": "<%= it.author %>",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "<%= it.kebabCase(it.name) %>": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "node dist/cli.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "cac": "^7.0.1",
    "consola": "^3.2.3"
  },
  "devDependencies": {
    "@types/node": "^22.17.1",
    "eslint": "^9.18.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vitest": "^2.3.5"
  },
  "engines": {
    "node": ">=18"
  }
}
