{
  "name": "TypeScript Library",
  "description": "A TypeScript library template with modern tooling and testing setup",
  "version": "1.0.0",
  "author": "bfra.me",
  "tags": ["library", "typescript", "npm", "package"],
  "variables": [
    {
      "name": "name",
      "description": "The name of the library",
      "type": "string",
