{
  "name": "@bfra.me/create",
  "description": "bfra.me create commands",
  "version": "0.0.0",
  "author": "Marcus R. Brown @igetgames",
  "bin": {
    "create": "./bin/run"
  },
  "bugs": "https://github.com/bfra-me/works/issues",
  "dependencies": {
    "@oclif/command": "1.8.22",
    "@oclif/config": "1.18.8",
    "@oclif/plugin-help": "3.3.1",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.26.10",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.3",
    "globby": "10.0.2"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/bfra-me/works",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "create",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "bfra-me/works",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint . --ext .ts",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "eslintConfig": {
    "extends": [
      "oclif",
      "oclif-typescript"
    ]
  }
}
