{
  "name": "@bfra.me/create",
  "version": "0.4.4",
  "description": "A command line utility to create new packages from customizable templates.",
  "keywords": [
    "cli",
    "create",
    "template"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfra-me/works.git",
    "directory": "packages/create"
  },
  "license": "MIT",
  "author": "Marcus R. Brown <contact@bfra.me>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "create": "./dist/cli.js"
  },
  "files": [
    "src",
    "dist",
    "!**/*.map"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsx src/cli.ts",
    "prepack": "pnpm run build",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@clack/prompts": "0.10.1",
    "@sxzz/create": "0.14.6",
    "cac": "6.7.14",
    "consola": "3.4.2"
  },
  "devDependencies": {
    "memfs": "4.17.1",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
